CREATE DATABASE  IF NOT EXISTS `trading`;
USE `trading`;
DROP TABLE IF EXISTS PLAYER_WALLET;
create table PLAYER_WALLET( 
   PLAYER_ID BIGINT(15) unsigned NOT NULL COMMENT '用户编号',
   SKU_CURRENCY_TYPE BIGINT(15)  COMMENT '货币类型',
   SKU_CURRENCY_VALUE BIGINT(15) COMMENT '货币数值',
   primary Key (PLAYER_ID,SKU_CURRENCY_TYPE) 
)ENGINE=InnoDB DEFAULT CHARSET=utf COMMENT='用户钱包表';

   
